aption: \"Найти компнаию для прогулки\" },\r\n      ]\r\n      title = 'FurBook'\r\n      color = 'green'\r\n      break\r\n    case 'chat':\r\n      links = [\r\n        { to: \"/main\", caption: \"Главное меню\" }\r\n      ]\r\n      title = 'FurBook Messenger'\r\n      color = 'yellow'\r\n      break\r\n    case 'locator':\r\n      links = [\r\n        { to: \"/profile/me\", caption: \"Главное меню\" }\r\n      ]\r\n      title = 'Locator'\r\n      color = 'red'\r\n      break\r\n    default:\r\n      links = []\r\n      title = 'T1vz'\r\n      color = 'green'\r\n  }\r\n  return (\r\n    <nav>\r\n      <div className={`nav-wrapper ${color} darken-1`} style={{ padding: '0 2rem' }}>\r\n        <a href=\"#\" className=\"brand-logo center\">{title}</a>\r\n        <a href=\"/\" className=\"right\" onClick={logoutHandler}>Выйти</a>\r\n        <ul id=\"nav-mobile\" className=\"left hide-on-med-and-down\">\r\n          {links.map(link => {\r\n            return <li><NavLink to={link.to}>{link.caption}</NavLink></li>\r\n          })}\r\n        </ul>\r\n      </div>\r\n    </nav>\r\n  )\r\n}","D:\\Work\\FurBook_Fullstack\\client\\src\\redux\\gameReducer.js",[],"D:\\Work\\FurBook_Fullstack\\client\\src\\pages\\AuthPage.js",["210","211"],"import React, { useContext, useEffect, useState } from 'react'\r\nimport { AuthContext } from '../context/AuthContext'\r\nimport { useHttp } from '../hooks/http.hook'\r\nimport { useMessage } from '../hooks/message.hook'\r\n\r\nexport const AuthPage = () => {\r\n    const auth = useContext(AuthContext)\r\n    const {loading, error, request, clearError} = useHttp()\r\n    const message = useMessage()\r\n    const [form, setForm] = useState({email:'', password:'', username:''})\r\n    const [isRegister, setIsRegister] = useState(false)\r\n\r\n    useEffect(()=>{\r\n        message(error)\r\n        clearError()\r\n    }, [error, message, clearError])\r\n\r\n    useEffect(()=>{\r\n        window.M.updateTextFields()\r\n    }, [])\r\n    \r\n 